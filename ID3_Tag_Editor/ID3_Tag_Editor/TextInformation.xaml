                <TextBox x:Name="tbSubtitle" HorizontalAlignment="Left" Height="23" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="255" Grid.Column="1" Grid.Row="2"/>
                <TextBox x:Name="tbTitleSortOrder" HorizontalAlignment="Left" Height="23" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="255" Grid.Column="1" Grid.Row="1"/>
                <ComboBox x:Name="cbLanguage" Grid.Column="1" HorizontalAlignment="Left" Margin="10,10,0,0" Grid.Row="6" VerticalAlignment="Top" Width="255"/>
                <ComboBox x:Name="cbGenre" Grid.Column="1" HorizontalAlignment="Left" Margin="10,10,0,0" Grid.Row="8" VerticalAlignment="Top" Width="255"/>
                <ComboBox x:Name="cbMood" Grid.Column="1" HorizontalAlignment="Left" Margin="10,10,0,0" Grid.Row="7" VerticalAlignment="Top" Width="255"/>
                <ComboBox x:Name="cbInitialKey" Grid.Column="1" HorizontalAlignment="Left" Margin="10,10,0,0" Grid.Row="10" VerticalAlignment="Top" Width="255"/>
